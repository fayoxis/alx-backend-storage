// Update all documents with "name" field equal to "Holberton school"
// Set the "name" field to "Holberton school" and add an "address"
db.school.update(
  { "name": "Holberton school" },
  {
    $set: {
      "name": "Holberton school",
      "address": "972 Mission street"
    }
  },
  { multi: true }
);
